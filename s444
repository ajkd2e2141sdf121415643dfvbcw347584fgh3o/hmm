local Player = game.Players.LocalPlayer
local SettingsPath = "VectorHub/Blox Fruits/" .. Player.Name .. ".json"

local bind = Instance.new("BindableFunction")
bind.OnInvoke = function(choice)
	if choice == "Yes" then
		if isfile(SettingsPath) then
			local success, data = pcall(function()
				return HttpService:JSONDecode(readfile(SettingsPath))
			end)

			if success and type(data) == "table" then
				for k, v in pairs(data) do
					if v == true then
						data[k] = false
						_G.Settings[k] = false
					end
				end

				-- ‡πÄ‡∏ã‡∏ü‡∏Å‡∏•‡∏±‡∏ö‡∏•‡∏á‡πÑ‡∏ü‡∏•‡πå
				writefile(SettingsPath, HttpService:JSONEncode(data))

				StarterGui:SetCore("SendNotification", {
					Title = "VectorHub",
					Text = "‚úÖ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!\nSuccess",
					Icon = "rbxassetid://14645512457",
					Duration = 5
				})
			else
				StarterGui:SetCore("SendNotification", {
					Title = "VectorHub",
					Text = "‚ö†Ô∏è ‡πÇ‡∏´‡∏•‡∏î‡πÑ‡∏ü‡∏•‡πå‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à\nNot Success",
					Icon = "rbxassetid://14645512457",
					Duration = 5
				})
			end
		else
			StarterGui:SetCore("SendNotification", {
				Title = "VectorHub",
				Text = "‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏ü‡∏•‡πå‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤\nNo Settings",
				Icon = "rbxassetid://14645512457",
				Duration = 5
			})
		end
	else
		StarterGui:SetCore("SendNotification", {
			Title = "VectorHub",
			Text = "‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤",
			Icon = "rbxassetid://14645512457",
