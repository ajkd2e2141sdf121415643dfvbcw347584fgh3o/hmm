function functionitem:Textbox(...)
    local options = ParseArguments(...)
    local Name = options.Name
    local Placeholder = options.Value or ""
    local Callback = options.Callback or function() end
    local Flag = options.Flag

    local Textbox = Instance.new("Frame")
    local UICorner_16 = Instance.new("UICorner")
    local Text_5 = Instance.new("TextLabel")
    local TextboxHoler = Instance.new("Frame")
    local UICorner_17 = Instance.new("UICorner")
    local HeadTitle = Instance.new("TextBox")

    Textbox.Name = Name
    Textbox.Parent = SectionContainer
    Textbox.BackgroundColor3 = Color3.fromRGB(1, 2, 3)
    Textbox.BackgroundTransparency = 0.700
    Textbox.BorderSizePixel = 0
    Textbox.ClipsDescendants = true
    Textbox.Size = UDim2.new(0, 265, 0, 60)
    Textbox.ZIndex = 16

    local UIStroke96 = Instance.new("UIStroke")
    UIStroke96.Thickness = 1.2
    UIStroke96.Parent = Textbox
    UIStroke96.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    UIStroke96.LineJoinMode = Enum.LineJoinMode.Round
    UIStroke96.Color = _G.Color
    UIStroke96.Transparency = 1

    Textbox.MouseEnter:Connect(function()
        TweenService:Create(
            UIStroke96,
            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Transparency = 0.10}
        ):Play()
    end)

    Textbox.MouseLeave:Connect(function()
        TweenService:Create(
            UIStroke96,
            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Transparency = 1}
        ):Play()
    end)

    UICorner_16.CornerRadius = UDim.new(0, 4)
    UICorner_16.Parent = Textbox

    Text_5.Name = "Text"
    Text_5.Parent = Textbox
    Text_5.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Text_5.BackgroundTransparency = 1.000
    Text_5.Position = UDim2.new(0, 10, 0, 10)
    Text_5.Size = UDim2.new(0, 43, 0, 12)
    Text_5.ZIndex = 16
    Text_5.Font = Enum.Font.GothamBold
    Text_5.Text = Name
    Text_5.TextColor3 = _G.Color
    Text_5.TextSize = 11.000
    Text_5.TextXAlignment = Enum.TextXAlignment.Left

    TextboxHoler.Name = "TextboxHoler"
    TextboxHoler.Parent = Textbox
    TextboxHoler.AnchorPoint = Vector2.new(0.5, 0.5)
    TextboxHoler.BackgroundColor3 = Color3.fromRGB(13, 13, 15)
    TextboxHoler.BackgroundTransparency = 1.000
    TextboxHoler.BorderSizePixel = 0
    TextboxHoler.Position = UDim2.new(0.5, 0, 0.5, 13)
    TextboxHoler.Size = UDim2.new(0.970000029, 0, 0, 30)

    UICorner_17.CornerRadius = UDim.new(0, 8)
    UICorner_17.Parent = TextboxHoler

    HeadTitle.Name = "HeadTitle"
    HeadTitle.Parent = TextboxHoler
    HeadTitle.AnchorPoint = Vector2.new(0.5, 0.5)
    HeadTitle.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
    HeadTitle.BackgroundTransparency = 1.000
    HeadTitle.BorderSizePixel = 0
    HeadTitle.ClipsDescendants = true
    HeadTitle.Position = UDim2.new(0.5, 3, 0.5, 0)
    HeadTitle.Size = UDim2.new(0.949999988, 0, 0, 25)
    HeadTitle.ZIndex = 16
    HeadTitle.Font = Enum.Font.GothamBold
    HeadTitle.PlaceholderColor3 = Color3.fromRGB(255, 255, 255)
    HeadTitle.PlaceholderText = Placeholder
    HeadTitle.Text = ""
    HeadTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    HeadTitle.TextSize = 13.000
    HeadTitle.TextXAlignment = Enum.TextXAlignment.Center

    local ButtonColor44 = Instance.new("UIStroke")
    ButtonColor44.Thickness = 0.9
    ButtonColor44.Name = ""
    ButtonColor44.Parent = HeadTitle
    ButtonColor44.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    ButtonColor44.LineJoinMode = Enum.LineJoinMode.Round
    ButtonColor44.Color = _G.Color
    ButtonColor44.Transparency = 0.2

    Textbox.MouseEnter:Connect(function()
        TweenService:Create(
            ButtonColor44,
            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Thickness = 1}
        ):Play()
        TweenService:Create(
            ButtonColor44,
            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Transparency = 0.5}
        ):Play()
    end)

    Textbox.MouseLeave:Connect(function()
        TweenService:Create(
            ButtonColor44,
            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Thickness = 0.9}
        ):Play()
        TweenService:Create(
            ButtonColor44,
            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Transparency = 0.2}
        ):Play()
    end)

    HeadTitle.FocusLost:Connect(function(ep)
        if ep then
            if #HeadTitle.Text > 0 then
                Callback(HeadTitle.Text)
                HeadTitle.Text = HeadTitle.Text
            end
        end
    end)

    -- üî• Register to Search
    RegisterComponent("Textbox", Name, Textbox)
    
    return
end
-- ==========================================
-- üî• IMPROVED Dropdown Function
-- ==========================================
function functionitem:Dropdown(...)
    local options = ParseArguments(...)
    local Name = options.Name or "Dropdown"
    local default = options.Value or ""
    local list = options.List or {}
    local Callback = options.Callback or function() end

    local Dropfunc = {}
    local currentValue = default
    local selectedButton = nil

    -- === MAIN CONTAINER ===
    local MainDropDown = Instance.new("Frame")
    local UICorner_7 = Instance.new("UICorner")
    local Header = Instance.new("Frame")
    local UICorner_8 = Instance.new("UICorner")
    local ClickArea = Instance.new("TextButton")
    local NameLabel = Instance.new("TextLabel")
    local ValueLabel = Instance.new("TextLabel")
    local Arrow = Instance.new("ImageButton")
    local SearchBox = Instance.new("TextBox")
    local SearchCorner = Instance.new("UICorner")
    local Scroll_Items = Instance.new("ScrollingFrame")
    local UIListLayout = Instance.new("UIListLayout")
    local UIPadding = Instance.new("UIPadding")

    -- === Setup Container ===
    MainDropDown.Name = Name
    MainDropDown.Parent = SectionContainer
    MainDropDown.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    MainDropDown.BackgroundTransparency = 0.7
    MainDropDown.BorderSizePixel = 0
    MainDropDown.ClipsDescendants = true
    MainDropDown.Size = UDim2.new(0, 265, 0, 35)
    MainDropDown.ZIndex = 16

    local BorderStroke = Instance.new("UIStroke")
    BorderStroke.Thickness = 1.2
    BorderStroke.Parent = MainDropDown
    BorderStroke.Color = _G.Color
    BorderStroke.Transparency = 1
    BorderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    BorderStroke.LineJoinMode = Enum.LineJoinMode.Round

    MainDropDown.MouseEnter:Connect(function()
        TweenService:Create(BorderStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {Transparency = 0.1}):Play()
    end)
    MainDropDown.MouseLeave:Connect(function()
        TweenService:Create(BorderStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {Transparency = 1}):Play()
    end)

    UICorner_7.CornerRadius = UDim.new(0, 4)
    UICorner_7.Parent = MainDropDown

    -- === Header Frame ===
    Header.Name = "Header"
    Header.Parent = MainDropDown
    Header.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    Header.BackgroundTransparency = 0.7
    Header.Size = UDim2.new(1, 0, 0, 35)
    Header.ZIndex = 16

    UICorner_8.CornerRadius = UDim.new(0, 4)
    UICorner_8.Parent = Header

    ClickArea.Name = "ClickArea"
    ClickArea.Parent = Header
    ClickArea.BackgroundTransparency = 1
    ClickArea.Size = UDim2.new(1, 0, 1, 0)
    ClickArea.ZIndex = 16
    ClickArea.AutoButtonColor = false
    ClickArea.Text = ""

    -- === Name Label (Left Side) ===
    NameLabel.Name = "NameLabel"
    NameLabel.Parent = Header
    NameLabel.BackgroundTransparency = 1
    NameLabel.Size = UDim2.new(0.5, -20, 0, 35)
    NameLabel.Position = UDim2.new(0, 10, 0, 0)
    NameLabel.ZIndex = 16
    NameLabel.Font = Enum.Font.GothamBold
    NameLabel.Text = Name
    NameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    NameLabel.TextSize = 13
    NameLabel.TextXAlignment = Enum.TextXAlignment.Left
    NameLabel.TextYAlignment = Enum.TextYAlignment.Center
    NameLabel.TextTransparency = 0.2

    -- === Value Label (Right Side) ===
    ValueLabel.Name = "ValueLabel"
    ValueLabel.Parent = Header
    ValueLabel.BackgroundTransparency = 1
    ValueLabel.Size = UDim2.new(0.5, -20, 0, 35)
    ValueLabel.Position = UDim2.new(0.5, 10, 0, 0)
    ValueLabel.ZIndex = 16
    ValueLabel.Font = Enum.Font.GothamBold
    ValueLabel.Text = (default ~= "") and default or "Select..."
    ValueLabel.TextColor3 = (default ~= "") and Color3.fromRGB(0, 200, 200) or Color3.fromRGB(150, 150, 150)
    ValueLabel.TextSize = 13
    ValueLabel.TextXAlignment = Enum.TextXAlignment.Left
    ValueLabel.TextYAlignment = Enum.TextYAlignment.Center
    ValueLabel.ClipsDescendants = true
    ValueLabel.TextTruncate = Enum.TextTruncate.AtEnd

    -- === Arrow Button ===
    Arrow.Parent = Header
    Arrow.BackgroundTransparency = 1
    Arrow.AnchorPoint = Vector2.new(1, 0.5)
    Arrow.Position = UDim2.new(1, -10, 0.5, 0)
    Arrow.Size = UDim2.new(0, 13, 0, 13)
    Arrow.ZIndex = 16
    Arrow.Image = "http://www.roblox.com/asset/?id=6282522798"
    Arrow.ImageColor3 = Color3.fromRGB(255, 255, 255)

    -- === Search Box ===
    SearchBox.Name = "SearchBox"
    SearchBox.Parent = MainDropDown
    SearchBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    SearchBox.Position = UDim2.new(0, 10, 0, 40)
    SearchBox.Size = UDim2.new(0, 245, 0, 30)
    SearchBox.ZIndex = 17
    SearchBox.Font = Enum.Font.GothamBold
    SearchBox.PlaceholderText = "üîç Search..."
    SearchBox.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    SearchBox.Text = ""
    SearchBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    SearchBox.TextSize = 12
    SearchBox.TextXAlignment = Enum.TextXAlignment.Left
    SearchBox.Visible = false

    local SearchPadding = Instance.new("UIPadding")
    SearchPadding.Parent = SearchBox
    SearchPadding.PaddingLeft = UDim.new(0, 10)

    SearchCorner.CornerRadius = UDim.new(0, 4)
    SearchCorner.Parent = SearchBox

    local SearchStroke = Instance.new("UIStroke")
    SearchStroke.Thickness = 1
    SearchStroke.Parent = SearchBox
    SearchStroke.Color = _G.Color
    SearchStroke.Transparency = 0.5

    -- === Scroll Items ===
    Scroll_Items.Name = "Items"
    Scroll_Items.Parent = MainDropDown
    Scroll_Items.BackgroundTransparency = 1
    Scroll_Items.BorderSizePixel = 0
    Scroll_Items.Position = UDim2.new(0, 0, 0, 75)
    Scroll_Items.Size = UDim2.new(1, 0, 0, 224)
    Scroll_Items.CanvasSize = UDim2.new(0, 0, 0, 0)
    Scroll_Items.ScrollBarThickness = 2
    Scroll_Items.Visible = false
    Scroll_Items.ZIndex = 17

    UIListLayout.Parent = Scroll_Items
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 5)
    UIListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Scroll_Items.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 10)
    end)

    UIPadding.Parent = Scroll_Items
    UIPadding.PaddingLeft = UDim.new(0, 10)
    UIPadding.PaddingTop = UDim.new(0, 5)

    -- === Toggle Panel ===
    local isOpen = false
    function Dropfunc:TogglePanel(state)
        isOpen = state
        TweenService:Create(MainDropDown, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = state and UDim2.new(0, 265, 0, 340) or UDim2.new(0, 265, 0, 35)
        }):Play()
        
        TweenService:Create(Arrow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Rotation = state and 180 or 0
        }):Play()

        SearchBox.Visible = state
        Scroll_Items.Visible = state
        
        if state then
            task.wait(0.1)
            SearchBox:CaptureFocus()
        else
            SearchBox.Text = ""
            Dropfunc:FilterItems("")
        end
    end

    ClickArea.MouseButton1Click:Connect(function()
        Dropfunc:TogglePanel(not isOpen)
    end)

    Arrow.MouseButton1Click:Connect(function()
        Dropfunc:TogglePanel(not isOpen)
    end)

    -- === Filter Items ===
    function Dropfunc:FilterItems(query)
        query = query:lower()
        for _, item in Scroll_Items:GetChildren() do
            if item:IsA("TextButton") then
                local matches = query == "" or item.Text:lower():find(query, 1, true)
                item.Visible = matches
            end
        end
    end

    SearchBox:GetPropertyChangedSignal("Text"):Connect(function()
        Dropfunc:FilterItems(SearchBox.Text)
    end)

    -- === Add Item ===
    function Dropfunc:Add(Text)
        local btn = Instance.new("TextButton")
        local corner = Instance.new("UICorner")
        local stroke = Instance.new("UIStroke")

        btn.Name = Text
        btn.Parent = Scroll_Items
        btn.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        btn.Size = UDim2.new(1, -10, 0, 30)
        btn.ZIndex = 17
        btn.Font = Enum.Font.GothamBold
        btn.Text = Text
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.TextSize = 12
        btn.AutoButtonColor = false
        btn.TextTruncate = Enum.TextTruncate.AtEnd

        corner.CornerRadius = UDim.new(0, 4)
        corner.Parent = btn

        stroke.Thickness = 1.2
        stroke.Parent = btn
        stroke.Color = _G.Color
        stroke.Transparency = 1
        stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        stroke.LineJoinMode = Enum.LineJoinMode.Round

        btn.MouseEnter:Connect(function()
            TweenService:Create(stroke, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {Transparency = 0.1}):Play()
            if btn.TextColor3 ~= Color3.fromRGB(0, 200, 200) then
                TweenService:Create(btn, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
                    BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                }):Play()
            end
        end)

        btn.MouseLeave:Connect(function()
            TweenService:Create(stroke, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {Transparency = 1}):Play()
            if btn.TextColor3 ~= Color3.fromRGB(0, 200, 200) then
                TweenService:Create(btn, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
                    BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                }):Play()
            end
        end)

        btn.MouseButton1Click:Connect(function()
            -- Check if clicking same button again (toggle off)
            if selectedButton == btn then
                -- Deselect
                TweenService:Create(selectedButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                    TextColor3 = Color3.fromRGB(255, 255, 255),
                    BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                }):Play()
                
                currentValue = ""
                selectedButton = nil
                ValueLabel.Text = "None"
                ValueLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
                
                pcall(Callback, "")
                task.wait(0.1)
                Dropfunc:TogglePanel(false)
                return
            end

            -- Reset old button
            if selectedButton then
                TweenService:Create(selectedButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                    TextColor3 = Color3.fromRGB(255, 255, 255),
                    BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                }):Play()
            end

            -- Set new value
            currentValue = Text
            selectedButton = btn
            
            -- Update display
            ValueLabel.Text = Text
            ValueLabel.TextColor3 = Color3.fromRGB(0, 200, 200)
            
            -- Highlight selected
            TweenService:Create(btn, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                TextColor3 = Color3.fromRGB(0, 200, 200),
                BackgroundColor3 = Color3.fromRGB(35, 35, 35)
            }):Play()
            
            pcall(Callback, Text)
            task.wait(0.1)
            Dropfunc:TogglePanel(false)
        end)

        -- Set default
        if Text == default then
            btn.TextColor3 = Color3.fromRGB(0, 200, 200)
            btn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
            selectedButton = btn
        end
    end

    -- === Add all items ===
    for _, item in ipairs(list) do
        Dropfunc:Add(item)
    end

    -- === Initialize ===
    if default ~= "" then
        pcall(Callback, default)
    end

    -- === External Functions ===
    function Dropfunc:Update(value)
        if value == "" or value == nil then
            -- Clear selection
            if selectedButton then
                TweenService:Create(selectedButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                    TextColor3 = Color3.fromRGB(255, 255, 255),
                    BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                }):Play()
            end
            currentValue = ""
            ValueLabel.Text = "None"
            ValueLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
            selectedButton = nil
            pcall(Callback, "")
        elseif table.find(list, value) then
            currentValue = value
            ValueLabel.Text = value
            ValueLabel.TextColor3 = Color3.fromRGB(0, 200, 200)
            pcall(Callback, value)
            
            -- Update button colors
            for _, btn in Scroll_Items:GetChildren() do
                if btn:IsA("TextButton") then
                    if btn.Text == value then
                        TweenService:Create(btn, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                            TextColor3 = Color3.fromRGB(0, 200, 200),
                            BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                        }):Play()
                        selectedButton = btn
                    else
                        TweenService:Create(btn, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                            TextColor3 = Color3.fromRGB(255, 255, 255),
                            BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                        }):Play()
                    end
                end
            end
        end
    end

    function Dropfunc:Get()
        return currentValue
    end

    function Dropfunc:Clear()
        for _, btn in Scroll_Items:GetChildren() do
            if btn:IsA("TextButton") then
                btn:Destroy()
            end
        end
        currentValue = ""
        ValueLabel.Text = "Select..."
        ValueLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
        SearchBox.Text = ""
        selectedButton = nil
        pcall(Callback, "")
    end

    function Dropfunc:SetList(newList)
        Dropfunc:Clear()
        list = newList
        for _, item in ipairs(list) do
            Dropfunc:Add(item)
        end
    end

    RegisterComponent("Dropdown", Name, MainDropDown)
    return Dropfunc
end
-- ==========================================
-- üî• IMPROVED MultiDropdown Function
-- ==========================================
function functionitem:MultiDropdown(...)
    local options = ParseArguments(...)
    local Name = options.Name or "MultiDropdown"
    local default = options.Value or {}
    local list = options.List or {}
    local Callback = options.Callback or function() end

    local Dropfunc = {}
    local selected = {}

    -- Load defaults
    if type(default) == "table" then
        for _, v in ipairs(default) do
            if not table.find(selected, v) then
                table.insert(selected, v)
            end
        end
    elseif type(default) == "string" and default ~= "" then
        table.insert(selected, default)
    end

    -- === MAIN CONTAINER ===
    local MainDropDown = Instance.new("Frame")
    local UICorner_7 = Instance.new("UICorner")
    local Header = Instance.new("Frame")
    local UICorner_8 = Instance.new("UICorner")
    local ClickArea = Instance.new("TextButton")
    local NameLabel = Instance.new("TextLabel")
    local ValueLabel = Instance.new("TextLabel")
    local Arrow = Instance.new("ImageButton")
    local SearchBox = Instance.new("TextBox")
    local SearchCorner = Instance.new("UICorner")
    local Scroll_Items = Instance.new("ScrollingFrame")
    local UIListLayout = Instance.new("UIListLayout")
    local UIPadding = Instance.new("UIPadding")

    -- === Setup Container ===
    MainDropDown.Name = Name
    MainDropDown.Parent = SectionContainer
    MainDropDown.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    MainDropDown.BackgroundTransparency = 0.7
    MainDropDown.BorderSizePixel = 0
    MainDropDown.ClipsDescendants = true
    MainDropDown.Size = UDim2.new(0, 265, 0, 35)
    MainDropDown.ZIndex = 16

    local BorderStroke = Instance.new("UIStroke")
    BorderStroke.Thickness = 1.2
    BorderStroke.Parent = MainDropDown
    BorderStroke.Color = _G.Color
    BorderStroke.Transparency = 1
    BorderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    BorderStroke.LineJoinMode = Enum.LineJoinMode.Round

    MainDropDown.MouseEnter:Connect(function()
        TweenService:Create(BorderStroke, TweenInfo.new(0.4), {Transparency = 0.1}):Play()
    end)
    MainDropDown.MouseLeave:Connect(function()
        TweenService:Create(BorderStroke, TweenInfo.new(0.4), {Transparency = 1}):Play()
    end)

    UICorner_7.CornerRadius = UDim.new(0, 4)
    UICorner_7.Parent = MainDropDown

    -- === Header Frame ===
    Header.Name = "Header"
    Header.Parent = MainDropDown
    Header.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    Header.BackgroundTransparency = 0.7
    Header.Size = UDim2.new(1, 0, 0, 35)
    Header.ZIndex = 16

    UICorner_8.CornerRadius = UDim.new(0, 4)
    UICorner_8.Parent = Header

    ClickArea.Name = "ClickArea"
    ClickArea.Parent = Header
    ClickArea.BackgroundTransparency = 1
    ClickArea.Size = UDim2.new(1, 0, 1, 0)
    ClickArea.ZIndex = 16
    ClickArea.AutoButtonColor = false
    ClickArea.Text = ""

    -- === Name Label ===
    NameLabel.Name = "NameLabel"
    NameLabel.Parent = Header
    NameLabel.BackgroundTransparency = 1
    NameLabel.Size = UDim2.new(0.5, -20, 0, 35)
    NameLabel.Position = UDim2.new(0, 10, 0, 0)
    NameLabel.ZIndex = 16
    NameLabel.Font = Enum.Font.GothamBold
    NameLabel.Text = Name
    NameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    NameLabel.TextSize = 13
    NameLabel.TextXAlignment = Enum.TextXAlignment.Left
    NameLabel.TextYAlignment = Enum.TextYAlignment.Center
    NameLabel.TextTransparency = 0.2

    -- === Value Label ===
    ValueLabel.Name = "ValueLabel"
    ValueLabel.Parent = Header
    ValueLabel.BackgroundTransparency = 1
    ValueLabel.Size = UDim2.new(0.5, -20, 0, 35)
    ValueLabel.Position = UDim2.new(0.5, 10, 0, 0)
    ValueLabel.ZIndex = 16
    ValueLabel.Font = Enum.Font.GothamBold
    ValueLabel.Text = (#selected > 0) and table.concat(selected, ", ") or "None"
    ValueLabel.TextColor3 = Color3.fromRGB(0, 200, 200)
    ValueLabel.TextSize = 13
    ValueLabel.TextXAlignment = Enum.TextXAlignment.Left
    ValueLabel.TextYAlignment = Enum.TextYAlignment.Center
    ValueLabel.ClipsDescendants = true

    -- === Arrow Button ===
    Arrow.Parent = Header
    Arrow.BackgroundTransparency = 1
    Arrow.AnchorPoint = Vector2.new(1, 0.5)
    Arrow.Position = UDim2.new(1, -10, 0.5, 0)
    Arrow.Size = UDim2.new(0, 13, 0, 13)
    Arrow.ZIndex = 16
    Arrow.Image = "http://www.roblox.com/asset/?id=6282522798"
    Arrow.ImageColor3 = Color3.fromRGB(255, 255, 255)

    -- === Search Box ===
    SearchBox.Name = "SearchBox"
    SearchBox.Parent = MainDropDown
    SearchBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    SearchBox.Position = UDim2.new(0, 10, 0, 40)
    SearchBox.Size = UDim2.new(0, 245, 0, 30)
    SearchBox.ZIndex = 17
    SearchBox.Font = Enum.Font.GothamBold
    SearchBox.PlaceholderText = "üîç Search..."
    SearchBox.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    SearchBox.Text = ""
    SearchBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    SearchBox.TextSize = 12
    SearchBox.TextXAlignment = Enum.TextXAlignment.Left
    SearchBox.Visible = false

    local SearchPadding = Instance.new("UIPadding")
    SearchPadding.Parent = SearchBox
    SearchPadding.PaddingLeft = UDim.new(0, 10)

    SearchCorner.CornerRadius = UDim.new(0, 4)
    SearchCorner.Parent = SearchBox

    local SearchStroke = Instance.new("UIStroke")
    SearchStroke.Thickness = 1
    SearchStroke.Parent = SearchBox
    SearchStroke.Color = _G.Color
    SearchStroke.Transparency = 0.5

    -- === Scroll Items ===
    Scroll_Items.Name = "Items"
    Scroll_Items.Parent = MainDropDown
    Scroll_Items.BackgroundTransparency = 1
    Scroll_Items.BorderSizePixel = 0
    Scroll_Items.Position = UDim2.new(0, 0, 0, 75)
    Scroll_Items.Size = UDim2.new(1, 0, 0, 224)
    Scroll_Items.CanvasSize = UDim2.new(0, 0, 0, 0)
    Scroll_Items.ScrollBarThickness = 2
    Scroll_Items.Visible = false
    Scroll_Items.ZIndex = 17

    UIListLayout.Parent = Scroll_Items
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 5)
    UIListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Scroll_Items.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 10)
    end)

    UIPadding.Parent = Scroll_Items
    UIPadding.PaddingLeft = UDim.new(0, 10)
    UIPadding.PaddingTop = UDim.new(0, 5)

    -- === Update Display Text ===
    local function updateDisplayText()
        if #selected > 0 then
            ValueLabel.Text = table.concat(selected, ", ")
            ValueLabel.TextColor3 = Color3.fromRGB(0, 200, 200)
        else
            ValueLabel.Text = "None"
            ValueLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
        end
    end

    -- === Toggle Panel ===
    local isOpen = false
    function Dropfunc:TogglePanel(state)
        isOpen = state
        TweenService:Create(MainDropDown, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            Size = state and UDim2.new(0, 265, 0, 340) or UDim2.new(0, 265, 0, 35)
        }):Play()
        TweenService:Create(Arrow, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            Rotation = state and 180 or 0
        }):Play()

        SearchBox.Visible = state
        Scroll_Items.Visible = state
        
        if state then
            task.wait(0.1)
            SearchBox:CaptureFocus()
        else
            SearchBox.Text = ""
            Dropfunc:FilterItems("")
        end
    end

    ClickArea.MouseButton1Click:Connect(function()
        Dropfunc:TogglePanel(not isOpen)
    end)

    Arrow.MouseButton1Click:Connect(function()
        Dropfunc:TogglePanel(not isOpen)
    end)

    -- === Filter Items ===
    function Dropfunc:FilterItems(query)
        query = query:lower()
        for _, item in Scroll_Items:GetChildren() do
            if item:IsA("TextButton") then
                local matches = query == "" or item.Text:lower():find(query, 1, true)
                item.Visible = matches
            end
        end
    end

    SearchBox:GetPropertyChangedSignal("Text"):Connect(function()
        Dropfunc:FilterItems(SearchBox.Text)
    end)

    -- === Add Item ===
    function Dropfunc:Add(Text)
        local btn = Instance.new("TextButton")
        local corner = Instance.new("UICorner")
        local stroke = Instance.new("UIStroke")

        btn.Name = Text
        btn.Parent = Scroll_Items
        btn.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        btn.Size = UDim2.new(1, -10, 0, 30)
        btn.ZIndex = 17
        btn.Font = Enum.Font.GothamBold
        btn.Text = Text
        btn.TextColor3 = table.find(selected, Text) and Color3.fromRGB(0, 200, 200) or Color3.fromRGB(255, 255, 255)
        btn.TextSize = 12
        btn.AutoButtonColor = false

        corner.CornerRadius = UDim.new(0, 4)
        corner.Parent = btn

        stroke.Thickness = 1.2
        stroke.Parent = btn
        stroke.Color = _G.Color
        stroke.Transparency = 1

        btn.MouseEnter:Connect(function()
            TweenService:Create(stroke, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {Transparency = 0.1}):Play()
        end)

        btn.MouseLeave:Connect(function()
            TweenService:Create(stroke, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {Transparency = 1}):Play()
        end)

        btn.MouseButton1Click:Connect(function()
            local foundIndex = table.find(selected, Text)
            if foundIndex then
                table.remove(selected, foundIndex)
                TweenService:Create(btn, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                    TextColor3 = Color3.fromRGB(255, 255, 255)
                }):Play()
            else
                table.insert(selected, Text)
                TweenService:Create(btn, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                    TextColor3 = Color3.fromRGB(0, 200, 200)
                }):Play()
            end
            updateDisplayText()
            Callback(selected)
        end)
    end

    -- === Add All Items ===
    for _, item in ipairs(list) do
        Dropfunc:Add(item)
    end

    updateDisplayText()
    if #selected > 0 then
        Callback(selected)
    end

    -- === External Functions ===
    function Dropfunc:Update(newTable)
        if type(newTable) == "table" then
            selected = {}
            for _, v in ipairs(newTable) do
                if table.find(list, v) then
                    table.insert(selected, v)
                end
            end
            updateDisplayText()
            Callback(selected)

            -- Update button colors
            for _, btn in Scroll_Items:GetChildren() do
                if btn:IsA("TextButton") then
                    local isSelected = table.find(selected, btn.Text)
                    TweenService:Create(btn, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
                        TextColor3 = isSelected and Color3.fromRGB(0, 200, 200) or Color3.fromRGB(255, 255, 255)
                    }):Play()
                end
            end
        end
    end

    function Dropfunc:Get()
        return selected
    end

    function Dropfunc:Clear()
        for _, btn in Scroll_Items:GetChildren() do
            if btn:IsA("TextButton") then
                btn:Destroy()
            end
        end
        selected = {}
        updateDisplayText()
        SearchBox.Text = ""
        Callback(selected)
    end

    function Dropfunc:Add(Text)
        -- This allows adding items dynamically after creation
        local btn = Instance.new("TextButton")
        local corner = Instance.new("UICorner")
        local stroke = Instance.new("UIStroke")

        btn.Name = Text
        btn.Parent = Scroll_Items
        btn.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        btn.Size = UDim2.new(1, -10, 0, 30)
        btn.ZIndex = 17
        btn.Font = Enum.Font.GothamBold
        btn.Text = Text
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.TextSize = 12
        btn.AutoButtonColor = false

        corner.CornerRadius = UDim.new(0, 4)
        corner.Parent = btn

        stroke.Thickness = 1.2
        stroke.Parent = btn
        stroke.Color = _G.Color
        stroke.Transparency = 1

        btn.MouseEnter:Connect(function()
            TweenService:Create(stroke, TweenInfo.new(0.4), {Transparency = 0.1}):Play()
        end)
        btn.MouseLeave:Connect(function()
            TweenService:Create(stroke, TweenInfo.new(0.4), {Transparency = 1}):Play()
        end)

        btn.MouseButton1Click:Connect(function()
            local foundIndex = table.find(selected, Text)
            if foundIndex then
                table.remove(selected, foundIndex)
                TweenService:Create(btn, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
            else
                table.insert(selected, Text)
                TweenService:Create(btn, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(0, 200, 200)}):Play()
            end
            updateDisplayText()
            Callback(selected)
        end)
    end

    RegisterComponent("MultiDropdown", Name, MainDropDown)
    return Dropfunc
end
-- ==========================================
-- üî• Slider Function
-- ==========================================
function functionitem:Slider(...)
    local options = ParseArguments(...)
    local text = options.Name
    local floor = options.floor or false  -- ‡∏ñ‡πâ‡∏≤ true = ‡∏ï‡∏±‡∏î‡∏ó‡∏®‡∏ô‡∏¥‡∏¢‡∏°
    local de = options.Value or 1
    local min = options.Min or 0
    local max = options.Max or math.huge
    local Callback = options.Callback or function() end
    local Flag = options.Flag
    local precision = options.Precision or 2  -- ‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏®‡∏ô‡∏¥‡∏¢‡∏° (‡πÉ‡∏´‡∏°‡πà!)

    -- ‡πÅ‡∏õ‡∏•‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÉ‡∏´‡πâ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏ä‡πà‡∏ß‡∏á min-max
    de = math.clamp(de, min, max)

    local SliderFrame = Instance.new("Frame")
    local LabelNameSlider = Instance.new("TextLabel")
    local ShowValueFrame = Instance.new("Frame")
    local CustomValue = Instance.new("TextBox")
    local ShowValueFrameUICorner = Instance.new("UICorner")
    local ValueFrame = Instance.new("Frame")
    local ValueFrameUICorner = Instance.new("UICorner")
    local PartValue = Instance.new("Frame")
    local PartValueUICorner = Instance.new("UICorner")
    local MainValue = Instance.new("Frame")
    local MainValueUICorner = Instance.new("UICorner")
    local ConneValue = Instance.new("Frame")
    local ConneValueUICorner = Instance.new("UICorner")
    local sliderfunc = {}

    -- === UI Setup (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°) ===
    SliderFrame.Name = text
    SliderFrame.Parent = SectionContainer
    SliderFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    SliderFrame.BackgroundTransparency = 0.7
    SliderFrame.Size = UDim2.new(0, 265, 0, 45)

    local UIStroke96 = Instance.new("UIStroke")
    UIStroke96.Thickness = 1.2
    UIStroke96.Parent = SliderFrame
    UIStroke96.Color = _G.Color
    UIStroke96.Transparency = 1
    UIStroke96.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    UIStroke96.LineJoinMode = Enum.LineJoinMode.Round

    SliderFrame.MouseEnter:Connect(function()
        TweenService:Create(UIStroke96, TweenInfo.new(0.4), {Transparency = 0.1}):Play()
    end)
    SliderFrame.MouseLeave:Connect(function()
        TweenService:Create(UIStroke96, TweenInfo.new(0.4), {Transparency = 1}):Play()
    end)

    local UICorner_7 = Instance.new("UICorner")
    UICorner_7.CornerRadius = UDim.new(0, 4)
    UICorner_7.Parent = SliderFrame

    LabelNameSlider.Name = "LabelNameSlider"
    LabelNameSlider.Parent = SliderFrame
    LabelNameSlider.BackgroundTransparency = 1
    LabelNameSlider.Position = UDim2.new(0.07, 0, 0.04, 0)
    LabelNameSlider.Size = UDim2.new(0, 182, 0, 25)
    LabelNameSlider.Font = Enum.Font.GothamBold
    LabelNameSlider.Text = text
    LabelNameSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
    LabelNameSlider.TextSize = 11
    LabelNameSlider.TextXAlignment = Enum.TextXAlignment.Left

    ShowValueFrame.Name = "ShowValueFrame"
    ShowValueFrame.Parent = SliderFrame
    ShowValueFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    ShowValueFrame.Position = UDim2.new(0.73, 0, 0.07, 0)
    ShowValueFrame.Size = UDim2.new(0, 58, 0, 21)

    CustomValue.Name = "CustomValue"
    CustomValue.Parent = ShowValueFrame
    CustomValue.AnchorPoint = Vector2.new(0.5, 0.5)
    CustomValue.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    CustomValue.Position = UDim2.new(0.5, 0, 0.5, 0)
    CustomValue.Size = UDim2.new(0, 55, 0, 21)
    CustomValue.Font = Enum.Font.GothamBold
    CustomValue.TextColor3 = Color3.fromRGB(255, 255, 255)
    CustomValue.TextSize = 11
    CustomValue.ClearTextOnFocus = false

    local UIStroke965 = Instance.new("UIStroke")
    UIStroke965.Thickness = 1.2
    UIStroke965.Parent = CustomValue
    UIStroke965.Color = _G.Color
    UIStroke965.Transparency = 0.1

    ShowValueFrameUICorner.CornerRadius = UDim.new(0, 4)
    ShowValueFrameUICorner.Parent = ShowValueFrame

    ValueFrame.Name = "ValueFrame"
    ValueFrame.Parent = SliderFrame
    ValueFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    ValueFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    ValueFrame.Position = UDim2.new(0.5, 0, 0.8, 0)
    ValueFrame.Size = UDim2.new(0, 200, 0, 5)

    ValueFrameUICorner.CornerRadius = UDim.new(0, 30)
    ValueFrameUICorner.Parent = ValueFrame

    PartValue.Name = "PartValue"
    PartValue.Parent = ValueFrame
    PartValue.BackgroundTransparency = 1
    PartValue.Position = UDim2.new(0, 0, 0, 0)
    PartValue.Size = UDim2.new(1, 0, 1, 0)

    MainValue.Name = "MainValue"
    MainValue.Parent = ValueFrame
    MainValue.BackgroundColor3 = _G.Color
    MainValue.Size = UDim2.new((de - min) / (max - min), 0, 0, 5)
    MainValue.BorderSizePixel = 0

    MainValueUICorner.CornerRadius = UDim.new(0, 30)
    MainValueUICorner.Parent = MainValue

    ConneValue.Name = "ConneValue"
    ConneValue.Parent = PartValue
    ConneValue.AnchorPoint = Vector2.new(0.5, 0.5)
    ConneValue.BackgroundColor3 = _G.Color
    ConneValue.Position = UDim2.new((de - min) / (max - min), 0, 0.5, 0)
    ConneValue.Size = UDim2.new(0, 10, 0, 10)

    ConneValueUICorner = Instance.new("UICorner")
    ConneValueUICorner.CornerRadius = UDim.new(0, 10)
    ConneValueUICorner.Parent = ConneValue

    -- === ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏õ‡∏•‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏õ‡πá‡∏ô‡∏ó‡∏®‡∏ô‡∏¥‡∏¢‡∏° ===
    local function formatValue(val)
        if floor then
            return math.floor(val)
        else
            return tonumber(string.format("%." .. precision .. "f", val)) or val
        end
    end

    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    CustomValue.Text = tostring(formatValue(de))

    -- === ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏≤‡∏Å Slider ===
    local dragging = false
    local function updateSlider(input)
        local relativeX = math.clamp((input.Position.X - ValueFrame.AbsolutePosition.X) / ValueFrame.AbsoluteSize.X, 0, 1)
        local value = min + (relativeX * (max - min))
        value = formatValue(value)

        local percent = (value - min) / (max - min)
        MainValue:TweenSize(UDim2.new(percent, 0, 0, 5), "Out", "Sine", 0.2, true)
        ConneValue:TweenPosition(UDim2.new(percent, 0, 0.5, 0), "Out", "Sine", 0.2, true)
        CustomValue.Text = tostring(value)
        pcall(Callback, value)
    end

    -- === Event ‡∏•‡∏≤‡∏Å ===
    local function startDrag(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
        end
    end
    local function endDrag(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = false
        end
    end

    ConneValue.InputBegan:Connect(startDrag)
    ConneValue.InputEnded:Connect(endDrag)
    ValueFrame.InputBegan:Connect(startDrag)
    ValueFrame.InputEnded:Connect(endDrag)
    SliderFrame.InputBegan:Connect(startDrag)
    SliderFrame.InputEnded:Connect(endDrag)

    game:GetService("UserInputService").InputChanged:Connect(function(input)
         if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
            updateSlider(input)
        end
    end)

	
    -- === ‡∏õ‡πâ‡∏≠‡∏ô‡∏Ñ‡πà‡∏≤‡πÉ‡∏ô TextBox ===
    CustomValue.FocusLost:Connect(function(enterPressed)
        if not enterPressed then return end
        local input = tonumber(CustomValue.Text)
        if not input then
            CustomValue.Text = tostring(formatValue(de))
            return
        end

        input = math.clamp(input, min, max)
        input = formatValue(input)

        local percent = (input - min) / (max - min)
        MainValue:TweenSize(UDim2.new(percent, 0, 0, 5), "Out", "Sine", 0.2, true)
        ConneValue:TweenPosition(UDim2.new(percent, 0, 0.5, 0), "Out", "Sine", 0.2, true)
        CustomValue.Text = tostring(input)
        pcall(Callback, input)
    end)

    -- === ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏à‡∏≤‡∏Å‡∏†‡∏≤‡∏¢‡∏ô‡∏≠‡∏Å ===
    function sliderfunc:Update(value)
        value = math.clamp(value, min, max)
        value = formatValue(value)
        local percent = (value - min) / (max - min)
        MainValue:TweenSize(UDim2.new(percent, 0, 0, 5), "Out", "Sine", 0.2, true)
        ConneValue:TweenPosition(UDim2.new(percent, 0, 0.5, 0), "Out", "Sine", 0.2, true)
        CustomValue.Text = tostring(value)
        pcall(Callback, value)
    end

    RegisterComponent("Slider", text, SliderFrame)
    return sliderfunc
end

function functionitem:AddColorpicker(...)
    local options = ParseArguments(...)
    local text = options.Name or "Colorpicker"
    local preset = options.Value or Color3.fromRGB(255, 255, 255)
    local Callback = options.Callback or function() end

    local OldToggleColor = Color3.fromRGB(0, 0, 0)
    local OldColor = Color3.fromRGB(0, 0, 0)
    local OldColorSelectionPosition = nil
    local OldHueSelectionPosition = nil
    local ColorH, ColorS, ColorV = 1, 1, 1
    local RainbowColorPicker = false
    local ColorPickerInput = nil
    local ColorInput = nil
    local HueInput = nil

    local Colorpicker = Instance.new("Frame")
    local ColorpickerTitle = Instance.new("TextLabel")
    local ColorpickerFrameOutline = Instance.new("Frame")
    local ColorpickerFrameOutlineCorner = Instance.new("UICorner")
    local ColorpickerFrame = Instance.new("Frame")
    local ColorpickerFrameCorner = Instance.new("UICorner")
    local Color = Instance.new("ImageLabel")
    local ColorCorner = Instance.new("UICorner")
    local ColorSelection = Instance.new("ImageLabel")
    local Hue = Instance.new("ImageLabel")
    local HueCorner = Instance.new("UICorner")
    local HueGradient = Instance.new("UIGradient")
    local HueSelection = Instance.new("ImageLabel")
    local PresetClr = Instance.new("Frame")
    local PresetClrCorner = Instance.new("UICorner")

    Colorpicker.Name = "Colorpicker"
    Colorpicker.Parent = SectionContainer
    Colorpicker.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Colorpicker.BackgroundTransparency = 1.000
    Colorpicker.Position = UDim2.new(0.0895741582, 0, 0.474232763, 0)
    Colorpicker.Size = UDim2.new(0, 403, 0, 175)

    ColorpickerTitle.Name = "ColorpickerTitle"
    ColorpickerTitle.Parent = Colorpicker
    ColorpickerTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ColorpickerTitle.BackgroundTransparency = 1.000
    ColorpickerTitle.Position = UDim2.new(0, 5, 0, 0)
    ColorpickerTitle.Size = UDim2.new(0, 200, 0, 29)
    ColorpickerTitle.Font = Enum.Font.Gotham
    ColorpickerTitle.Text = text
    ColorpickerTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    ColorpickerTitle.TextSize = 14.000
    ColorpickerTitle.TextXAlignment = Enum.TextXAlignment.Left

    ColorpickerFrameOutline.Name = "ColorpickerFrameOutline"
    ColorpickerFrameOutline.Parent = ColorpickerTitle
    ColorpickerFrameOutline.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ColorpickerFrameOutline.Position = UDim2.new(-0.00100000005, 0, 0.991999984, 0)
    ColorpickerFrameOutline.Size = UDim2.new(0, 238, 0, 139)

    ColorpickerFrameOutlineCorner.CornerRadius = UDim.new(0, 3)
    ColorpickerFrameOutlineCorner.Name = "ColorpickerFrameOutlineCorner"
    ColorpickerFrameOutlineCorner.Parent = ColorpickerFrameOutline

    ColorpickerFrame.Name = "ColorpickerFrame"
    ColorpickerFrame.Parent = ColorpickerTitle
    ColorpickerFrame.BackgroundColor3 = Color3.fromRGB(23, 23, 23)
    ColorpickerFrame.ClipsDescendants = true
    ColorpickerFrame.Position = UDim2.new(0.00899999978, 0, 1.06638515, 0)
    ColorpickerFrame.Selectable = true
    ColorpickerFrame.Size = UDim2.new(0, 234, 0, 135)

    ColorpickerFrameCorner.CornerRadius = UDim.new(0, 3)
    ColorpickerFrameCorner.Name = "ColorpickerFrameCorner"
    ColorpickerFrameCorner.Parent = ColorpickerFrame

    Color.Name = "Color"
    Color.Parent = ColorpickerFrame
    Color.BackgroundColor3 = Color3.fromRGB(255, 0, 4)
    Color.Position = UDim2.new(0, 10, 0, 10)
    Color.Size = UDim2.new(0, 154, 0, 118)
    Color.ZIndex = 10
    Color.Image = "rbxassetid://4155801252"

    ColorCorner.CornerRadius = UDim.new(0, 3)
    ColorCorner.Name = "ColorCorner"
    ColorCorner.Parent = Color

    ColorSelection.Name = "ColorSelection"
    ColorSelection.Parent = Color
    ColorSelection.AnchorPoint = Vector2.new(0.5, 0.5)
    ColorSelection.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ColorSelection.BackgroundTransparency = 1.000
    ColorSelection.Position = UDim2.new(preset and select(3, Color3.toHSV(preset)) or 0.5, 0, 0.5, 0)
    ColorSelection.Size = UDim2.new(0, 18, 0, 18)
    ColorSelection.Image = "http://www.roblox.com/asset/?id=4805639000"
    ColorSelection.ScaleType = Enum.ScaleType.Fit

    Hue.Name = "Hue"
    Hue.Parent = ColorpickerFrame
    Hue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Hue.Position = UDim2.new(0, 171, 0, 10)
    Hue.Size = UDim2.new(0, 18, 0, 118)

    HueCorner.CornerRadius = UDim.new(0, 3)
    HueCorner.Name = "HueCorner"
    HueCorner.Parent = Hue

    HueGradient.Color = ColorSequence.new {
        ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 0, 4)),
        ColorSequenceKeypoint.new(0.20, Color3.fromRGB(234, 255, 0)),
        ColorSequenceKeypoint.new(0.40, Color3.fromRGB(21, 255, 0)),
        ColorSequenceKeypoint.new(0.60, Color3.fromRGB(0, 255, 255)),
        ColorSequenceKeypoint.new(0.80, Color3.fromRGB(0, 17, 255)),
        ColorSequenceKeypoint.new(0.90, Color3.fromRGB(255, 0, 251)),
        ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 0, 4))
    }
    HueGradient.Rotation = 270
    HueGradient.Name = "HueGradient"
    HueGradient.Parent = Hue

    HueSelection.Name = "HueSelection"
    HueSelection.Parent = Hue
    HueSelection.AnchorPoint = Vector2.new(0.5, 0.5)
    HueSelection.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    HueSelection.BackgroundTransparency = 1.000
    HueSelection.Position = UDim2.new(0.48, 0, preset and 1 - select(1, Color3.toHSV(preset)) or 0.5, 0)
    HueSelection.Size = UDim2.new(0, 18, 0, 18)
    HueSelection.Image = "http://www.roblox.com/asset/?id=4805639000"

    PresetClr.Name = "PresetClr"
    PresetClr.Parent = ColorpickerFrame
    PresetClr.BackgroundColor3 = preset
    PresetClr.Position = UDim2.new(0.846153855, 0, 0.0740740746, 0)
    PresetClr.Size = UDim2.new(0, 25, 0, 25)

    PresetClrCorner.CornerRadius = UDim.new(0, 3)
    PresetClrCorner.Name = "PresetClrCorner"
    PresetClrCorner.Parent = PresetClr

    local function UpdateColorPicker(nope)
        PresetClr.BackgroundColor3 = Color3.fromHSV(ColorH, ColorS, ColorV)
        Color.BackgroundColor3 = Color3.fromHSV(ColorH, 1, 1)
        UpdateColorDependents(PresetClr.BackgroundColor3) -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï _G.Color ‡πÅ‡∏•‡∏∞ components ‡∏≠‡∏∑‡πà‡∏ô‡πÜ
        pcall(Callback, PresetClr.BackgroundColor3)
    end

    ColorH = 1 - (math.clamp(HueSelection.AbsolutePosition.Y - Hue.AbsolutePosition.Y, 0, Hue.AbsoluteSize.Y) / Hue.AbsoluteSize.Y)
    ColorS = (math.clamp(ColorSelection.AbsolutePosition.X - Color.AbsolutePosition.X, 0, Color.AbsoluteSize.X) / Color.AbsoluteSize.X)
    ColorV = 1 - (math.clamp(ColorSelection.AbsolutePosition.Y - Color.AbsolutePosition.Y, 0, Color.AbsoluteSize.Y) / Color.AbsoluteSize.Y)

    PresetClr.BackgroundColor3 = preset
    Color.BackgroundColor3 = preset
    UpdateColorDependents(preset) -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÉ‡∏´‡πâ _G.Color ‡πÅ‡∏•‡∏∞ components
    pcall(Callback, PresetClr.BackgroundColor3)

    Color.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1  or input.UserInputType == Enum.UserInputType.Touch then
            if ColorInput then
                ColorInput:Disconnect()
            end
            ColorInput = game:GetService("RunService").RenderStepped:Connect(function()
                local ColorX = (math.clamp(game.Players.LocalPlayer:GetMouse().X - Color.AbsolutePosition.X, 0, Color.AbsoluteSize.X) / Color.AbsoluteSize.X)
                local ColorY = (math.clamp(game.Players.LocalPlayer:GetMouse().Y - Color.AbsolutePosition.Y, 0, Color.AbsoluteSize.Y) / Color.AbsoluteSize.Y)
                ColorSelection.Position = UDim2.new(ColorX, 0, ColorY, 0)
                ColorS = ColorX
                ColorV = 1 - ColorY
                UpdateColorPicker(true)
            end)
        end
    end)

    Color.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1  or input.UserInputType == Enum.UserInputType.Touch then
            if ColorInput then
                ColorInput:Disconnect()
            end
        end
    end)

    Hue.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1  or input.UserInputType == Enum.UserInputType.Touch then
            if HueInput then
                HueInput:Disconnect()
            end
            HueInput = game:GetService("RunService").RenderStepped:Connect(function()
                local HueY = (math.clamp(game.Players.LocalPlayer:GetMouse().Y - Hue.AbsolutePosition.Y, 0, Hue.AbsoluteSize.Y) / Hue.AbsoluteSize.Y)
                HueSelection.Position = UDim2.new(0.48, 0, HueY, 0)
                ColorH = 1 - HueY
                UpdateColorPicker(true)
            end)
        end
    end)

    Hue.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1  or input.UserInputType == Enum.UserInputType.Touch then
            if HueInput then
                HueInput:Disconnect()
            end
        end
    end)

RegisterComponent("Colorpicker", text, Colorpicker) -- ‡∏™‡∏°‡∏°‡∏ï‡∏¥‡∏ß‡πà‡∏≤ RegisterComponent ‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà
RegisterColorDependent("ColorpickerPreset", PresetClr, "BackgroundColor3") -- ‡∏•‡∏á‡∏ó‡∏∞‡πÄ‡∏ö‡∏µ‡∏¢‡∏ô PresetClr
RegisterColorDependent("UiToggle_UiStroke1", UiToggle_UiStroke1, "Color")
RegisterColorDependent("Title",Title,"TextColor3")
RegisterColorDependent("Title2",Title2,"TextColor3")
RegisterColorDependent("SearchResults",SearchResults,"ScrollBarImageColor3")
RegisterColorDependent("SearchStroke",SearchStroke,"Color")
RegisterColorDependent("FrameTab",FrameTab,"BackgroundColor3")
RegisterColorDependent("FrameTab",FrameTab,"BackgroundColor3")
RegisterColorDependent("FrameTab",FrameTab,"BackgroundColor3")
RegisterColorDependent("ResultsStroke",ResultsStroke,"Color")
RegisterColorDependent("UIStroke96",UIStroke96,"Color")
RegisterColorDependent("UIStroke96",UIStroke96,"Color")
RegisterColorDependent("UIStroke96",UIStroke96,"Color")
RegisterColorDependent("SearchIcon",SearchIcon,"ImageColor3")
RegisterColorDependent("ConneValue",ConneValue,"BackgroundColor3")
RegisterColorDependent("MainValue",MainValue,"BackgroundColor3")
RegisterColorDependent("UIStroke965",UIStroke965,"Color")
RegisterColorDependent("UIStroke96",UIStroke96,"Color")
RegisterColorDependent("SearchStroke",SearchStroke,"Color")
                end				
                return functionitem
			end
			return sections
		end
		return tabs
	end
return VectorHub
