            local buffer = IsOnMobile and 80 or 50  -- Mobile ‡∏ï‡πâ‡∏≠‡∏á‡πÉ‡∏´‡∏ç‡πà‡∏Å‡∏ß‡πà‡∏≤
            Left.CanvasSize = UDim2.new(0,0,0,LeftList.AbsoluteContentSize.Y + buffer)
        end)

        RightList:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            local buffer = IsOnMobile and 80 or 50
            Right.CanvasSize = UDim2.new(0,0,0,RightList.AbsoluteContentSize.Y + buffer)
        end)

