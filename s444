    local function updateSizes()
        if _G.CurrentLeft then
            _G.CurrentLeft.Size = UDim2.new(0, 274, 0, _G.SizePage)
        end
        if _G.CurrentRight then
            _G.CurrentRight.Size = UDim2.new(0, 274, 0, _G.SizePage)
        end
    end

